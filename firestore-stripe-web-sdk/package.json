{
  "name": "@stripe/firestore-stripe-payments",
  "version": "0.0.6",
  "description": "Client SDK for the firestore-stripe-payments Firebase Extension",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "api-documenter": "api-documenter markdown --input-folder temp --output-folder markdown",
    "api-extractor": "api-extractor run --verbose",
    "api-extractor:local": "npm run build && api-extractor run --local --verbose",
    "build": "tsc",
    "postbuild": "node ./postbuild.js",
    "test": "firebase emulators:exec --only firestore,auth --project fake-project-id 'npm run test:unit' && exit 0",
    "test:unit": "karma start --single-run"
  },
  "keywords": [
    "firebase",
    "payments",
    "subscriptions",
    "stripe"
  ],
  "author": "Stripe (https://stripe.com/)",
  "contributors": [
    "Firebase <firebase-support@google.com> (https://firebase.google.com/)"
  ],
  "license": "Apache-2.0",
  "files": [
    "lib/",
    "LICENSE",
    "NOTICE.txt",
    "README.md"
  ],
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.65",
    "@microsoft/api-extractor": "^7.18.17",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-like": "^1.1.1",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "10.0.6",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-like": "^1.1.1",
    "firebase-tools": "^9.18.0",
    "mocha": "^9.1.1",
    "karma": "^6.3.4",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-chrome-launcher": "^3.1.0",
    "webpack": "^4.46.0",
    "source-map-loader": "^1.1.3",
    "replace-in-file": "^6.2.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.2.1",
    "ts-loader": "^8.3.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@firebase/app": "^0.7.5",
    "@firebase/auth": "^0.17.2",
    "@firebase/firestore": "^3.2.0"
  }
}
